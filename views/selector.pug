extends layout

block content
  h1= title
  - let image_names = ['pal1.jpg','pal2.jpg','pal3.jpg','pal4.jpg','pal5.jpg']
  - for(let i=0;i<3;i++){
      - let rno = Math.floor(Math.random()*5)
      - let iname = image_names[rno]
      img(src=`images/${iname}` alt='palanquin' style='width:300px; height:300px;')
  - }
  
  
